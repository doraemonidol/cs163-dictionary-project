void ViewFavoriteList(TrieNode* root, string str, int level, Node FList[], int num)
{
    if (isLeafNode(root)) {
        str[level] = '\0';
        FList[num].word = str;
        FList[num].content = root->content;
        num++;
        //cout << str << " " << root->content << endl;
    }

    for (int i = 0; i < 128; i++) {
        if (root->children[i]) {
            str[level] = i;
            ViewFavoriteList(root->children[i], str, level + 1, FList, num);
        }
    }
}
